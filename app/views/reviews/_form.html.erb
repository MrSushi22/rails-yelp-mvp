<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content, placeholder: "Giv us a feedback" %>
  <%= f.input :rating, placeholder: "Rate us from 1-5", collection: (1..5) %>
  <%= f.submit "Add a review", class: "btn btn-primary" %>
  <%= link_to 'Back to Restaurants list', restaurants_path, class: "btn btn-primary" %>
<% end %>
